<!DOCTYPE html>
<html>
<head>
  <title>Gnarwhales</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if @current_user %>
	<%= render partial: '/navigation_user', :locals => {user: @current_user}%>
<% else %>
	<%= render partial: '/navigation_non_user' unless @disable_nav %>
<% end %>

<%= yield %>

</body>
</html>
